[flake8]
max-line-length = 140
ignore = E501,E203,E704,W503
exclude = .git,__pycache__,old,build,dist,venv,env,.env,.venv,migrations,app/register/entrypoints/view/money_format.py,app/reporter/utils.py
per-file-ignores =
    # Module level import not at top of file. This is due newrelic initialization.
    */app/__main__.py: E402